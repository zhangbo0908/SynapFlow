{
  "name": "synapflow",
  "version": "1.4.0",
  "description": "A native mind mapping tool powered by Electron and React",
  "main": "./out/main/index.js",
  "author": "SynapFlow Team",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "generate:icons": "electron-icon-builder --input=./resources/icon.png --output=build --flatten",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "clsx": "^2.0.0",
    "electron-updater": "^6.1.7",
    "html-to-image": "^1.11.13",
    "immer": "^11.1.3",
    "jspdf": "^4.1.0",
    "jszip": "^3.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.0.0",
    "uuid": "^9.0.1",
    "write-file-atomic": "^7.0.0",
    "xml2js": "^0.6.2",
    "zustand": "^5.0.11"
  },
  "build": {
    "appId": "com.synapflow.app",
    "productName": "SynapFlow",
    "publish": {
      "provider": "github",
      "owner": "zhangbo0908",
      "repo": "SynapFlow"
    },
    "mac": {
      "category": "public.app-category.productivity",
      "sign": false,
      "notarize": false,
      "hardenedRuntime": false,
      "gatekeeperAssess": false
    }
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/jspdf": "^1.3.3",
    "@types/node": "^18.19.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^9.0.8",
    "@types/write-file-atomic": "^4.0.3",
    "@types/xml2js": "^0.4.14",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-icon-builder": "^2.0.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "jsdom": "^28.0.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
